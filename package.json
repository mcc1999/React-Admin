{
  "name": "react-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite --mode dev",
    "test": "vite --mode test",
    "prod": "vite --mode prod",
    "build": "tsc -b && vite build --mode prod",
    "build:dev": "tsc -b && vite build --mode dev",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src --max-warnings 0",
    "lint:fix": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./src --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@ant-design/pro-components": "^2.7.15",
    "@loadable/component": "^5.16.4",
    "ahooks": "^3.8.0",
    "antd": "^5.19.4",
    "axios": "^1.7.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0"
  },
  "devDependencies": {
    "@types/loadable__component": "^5.13.9",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.40",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
